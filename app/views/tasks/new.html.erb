<div class="main-container">
  <div class="form-container">
    <%= simple_form_for [@category, @task], html: { autocomplete: "off" } do |f| %>
    <h5><%= f.input :name %></h5>
    <div class="add-task-container">
      <div class="link-container">
        <div class="link">
        <%= f.submit "Add task" %>
        </div>
      </div>
    </div>
    <% end %>
  </div>
</div>
